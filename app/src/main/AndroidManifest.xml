<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="net.yslibrary.realmperf">

    <application
            android:name=".App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".list.ListActivity">
        </activity>
        <activity android:name=".realmlist.RealmListActivity">
        </activity>
        <activity android:name=".standalonerealmlist.StandaloneRealmListActivity">
        </activity>
        <activity android:name=".standalonelist.StandaloneListActivity">
        </activity>
        <activity android:name=".pojo.PojoActivity">
        </activity>
    </application>

</manifest>