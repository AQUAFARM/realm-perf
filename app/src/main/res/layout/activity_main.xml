<?xml version="1.0" encoding="utf-8"?>
<layout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:orientation="vertical"
            tools:context="net.yslibrary.realmperf.MainActivity">

        <TextView
                android:id="@+id/note_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="loading..."/>

        <TextView
                android:id="@+id/required_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="loading..."/>

        <Button
                android:id="@+id/to_realm_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RealmList"/>

        <Button
                android:id="@+id/to_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="ArrayList&lt;RealmObject>"/>

        <Button
                android:id="@+id/to_realmlist_to_standalone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RealmList to Standalone"/>

        <Button
                android:id="@+id/to_standalone_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="ArrayList&lt;Standalone>"/>

        <Button
                android:id="@+id/to_pojo_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="ArrayList&lt;POJO>"/>
    </LinearLayout>
</layout>